#!/usr/bin/python3
dna4="ATCGATCGATCGATCGACTGACTAGTCATAGCTATGCATGTAGCTACTCGATCGATCGATCGATCGATCGATCGATCGATCGATCATGCTACTCGATCGATCGATCGATCGATCGATCGATCGATCGATCGATCATGCTATCATCGATCGATATCGATGCATCGACTACTAT"
exon1=dna4[:63]
exon2=dna4[90:]
coding=exon1=exon2
percentage=(len(coding)/len(dna4))*100
intron=dna4[63:90].lower()
dna4_sequence= exon1+intron+exon2
print(dna4_sequence)
